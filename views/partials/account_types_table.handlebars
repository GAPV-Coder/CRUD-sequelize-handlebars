<table class="table my-5">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Account Type:</th>
    </tr>
  </thead>
  <tbody>
      {{#each accountTypes}}
        <tr>
            <th scope="row">{{this.id}}</th>
            <td>{{this.type}}</td>
        </tr>
      {{/each}}
  </tbody>
</table>